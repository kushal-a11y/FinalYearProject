    "def normalize_education(x):\n",
    "    x = str(x).lower()\n",
    "    if \"phd\" in x or \"doctorate\" in x:\n",
    "        return \"PhD\"\n",
    "    elif \"master\" in x or \"postgraduate\" in x:\n",
    "        return \"Postgraduate\"\n",
    "    elif \"bachelor\" in x or \"graduate\" in x:\n",
    "        return \"Graduate\"\n",
    "    elif \"school\" in x or \"diploma\" in x:\n",
    "        return \"School\"\n",
    "    else:\n",
    "        return \"Other\"\n",
    "\n",
    "df[\"Education\"] = df[\"Education\"].apply(normalize_education)"
   ]

    "def infer_income(prof):\n",
    "    p = str(prof).lower()\n",
    "    if any(w in p for w in [\"doctor\", \"engineer\", \"scientist\", \"software\", \"it\", \"manager\"]):\n",
    "        return \"High\"\n",
    "    elif any(w in p for w in [\"teacher\", \"nurse\", \"clerk\", \"sales\", \"govt\"]):\n",
    "        return \"Medium\"\n",
    "    else:\n",
    "        return \"Low\"\n",
    "\n",
    "df[\"Income\"] = df[\"Profession\"].apply(infer_income)"
